#!ipxe
prompt --timeout 5000 --key y Connect to Wi-Fi? (Y/N)... && echo set SSID: && read ssid && echo set Key: && read key ||
:dhcp ifconf -c dhcp || config || goto dhcp
:menu chain https://raw.githubusercontent.com/Losenmann/SmartBoot/master/default.ipxe ||
console && prompt --key y Error loading menu, try again? (Y/N)... && goto menu || shell